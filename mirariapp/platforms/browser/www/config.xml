<?xml version='1.0' encoding='utf-8'?>
<widget id="app.mirari.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MirariApp</name>
    <description>
        mirari.app
    </description>
    <author email="g@gsutavo-castellanos.com" href="http://cordova.io">
        Gustavo Castellanos
    </author>
    <content src="dist/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
