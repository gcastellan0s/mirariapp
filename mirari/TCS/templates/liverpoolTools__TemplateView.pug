extends "generic/TemplateView.html"
-load static
-load i18n
-load mirari_tags

block subheader
    .kt-subheader__main
        h3.kt-subheader__title.kt-subheader__title--separator
            strong  HERRAMIENTAS LIVERPOOL
        span.kt-subheader__separator.kt-hidden
        .kt-subheader__breadcrumbs.mr-5
            a.kt-subheader__breadcrumbs-link(href='/')  INICIO

block content
    #kt_portlet.kt-portlet
        .kt-portlet__body
                    
            .form-group.form-group-last
                .alert.alert-secondary(role='alert')
                    .alert-icon
                        i.flaticon-warning.kt-font-brand
                    .alert-text
                        | The example form below demonstrates common HTML form elements that receive updated styles from Bootstrap with additional classes.
            .form-group.form-group-last
                label(for='exampleTextarea') Example textarea
                textarea#exampleTextarea.form-control(rows='3')

script.
    {%block created%}
        var KTCalendarBackgroundEvents = function() {
            return {
                init: function() {
                    
                }
            };
        }();
        jQuery(document).ready(function() {
            KTCalendarBackgroundEvents.init();
        });
    {%endblock created%}
