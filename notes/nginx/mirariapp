
upstream mirariapp_server {
  server unix:/webapps/mirariapp/run/gunicorn.sock fail_timeout=0;
}



server {
    server_name credipyme.mx www.credipyme.mx ;
    client_max_body_size 4G;
    access_log /webapps/mirariapp/logs/nginx-access.log;
    error_log /webapps/mirariapp/logs/nginx-error.log;
    location / {
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header Host $http_host;
        proxy_redirect off;
        if (!-f $request_filename) {
            proxy_pass http://mirariapp_server;
            break;
        }
    }
    error_page 500 502 503 504 /500.html;
    location = /500.html {
        root /webapps/mirariapp/static/;
    }
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/credipyme.mx-0001/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/credipyme.mx-0001/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot
}
server {
    if ($host = credipyme.mx) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    if ($host = www.credipyme.mx) {
        return 301 https://$host$request_uri;
    } # managed by Certbot
    listen 80;
    server_name credipyme.mx www.credipyme.mx;
    return 404;
}



